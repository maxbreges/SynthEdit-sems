<?xml version="1.0" encoding="UTF-8"?>
<PluginList>
    <Plugin id="FileDialog" name="File Dialog" category="mx/Sub-Controls">
        <GUI>
            <Pin name="File Name" datatype="string"/>
            <Pin name="File Extension" default="wav" datatype="string"/>
			<Pin name="Choice" datatype="int"/>
			<Pin name="Items List" datatype="string"/>			
            <Pin name="Trigger" datatype="bool" direction="out"/>
            <Pin name="Save Mode" datatype="bool"/>
			<Pin name="Directory" datatype="string" direction="out"/>
			<Pin name="Debug" datatype="string" direction="out" private="true"/>
        </GUI>
    </Plugin>
</PluginList>
