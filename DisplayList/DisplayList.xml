<?xml version="1.0" encoding="UTF-8"?>
<PluginList>
	<Plugin id="DisplayList" name="Display List" category="mx/Controls">
		<GUI graphicsApi="GmpiGui">
			<Pin name="Hint" datatype="string" isMinimised="true"/>
			<Pin name="BgColor" datatype="string" default="FF000000" private="true"/>
			<Pin name="Color" datatype="string" default="FF660000" private="false"/>
			<Pin name="Text" datatype="string" direction="out" private="true"/>
			<Pin name="Text Color" datatype="string" default="ffffffff" isMinimised="true"/>
			<Pin name="Text Font" datatype="string" default="Times New Roman" isMinimised="true"/>
			<Pin name="Font Size" datatype="int" default="16" isMinimised="true" />
			<Pin name="AnimPos+Shift" datatype="float" direction="in"/>
			<Pin name="AnimPos+Ctrl" datatype="float" direction="in"/>
			<Pin name="Choice" datatype="int" direction="in" />
			<Pin name="List Items" datatype="string" direction="in" />
			<Pin name="List Size" datatype="int" direction="in" />
			<Pin name="Mouse Down" datatype="bool" private="false"/>
			<Pin name="Shift+Drag" datatype="bool" private="false"/>
			<Pin name="Corner Radius" datatype="int" isMinimised="true" default="5"/>
		</GUI>
	</Plugin>
</PluginList>
