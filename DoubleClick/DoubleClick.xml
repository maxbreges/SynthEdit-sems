<?xml version="1.0" encoding="UTF-8"?>
<PluginList>
    <Plugin id="DoubleClick" name="Double Click" category="mx/Sub-Controls">
        <GUI>
            <Pin name="Mouse Down" datatype="bool" direction="out"/>
            <Pin name="Time ms" datatype="int" default="250"/>
            <Pin name="Countdown Out" datatype="int" direction="out" private="true"/>
			<Pin name="Click Count" datatype="int" direction="out" isMinimised="true"/>
            <Pin name="AnimPos" datatype="float" />
            <Pin name="Reset Value" datatype="float" default="0.5"/>
        </GUI>
    </Plugin>
</PluginList>
